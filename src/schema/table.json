{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "columns": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": ["string", "integer", "decimal", "boolean", "timestamp"]
          }
        },
        "required": ["name", "type"]
      }
    }
  },
  "required": ["name", "columns"]
}